{
    "name": "html5-bootstrap-editorial",
    "version": "1.0.0",
    "description": "HTML5 Bootstrap theme inspired by HTML5 UP Editorial template",
    "main": "index.html",
    "scripts": {
        "sass": "sass assets/scss/main.scss assets/css/main.css",
        "sass:watch": "sass --watch assets/scss/main.scss assets/css/main.css",
        "sass:compressed": "sass assets/scss/main.scss assets/css/main.css --style=compressed",
        "serve": "live-server --port=3000",
        "dev": "npm-run-all --parallel sass:watch serve"
    },
    "keywords": [
        "html5",
        "bootstrap",
        "sass",
        "template",
        "editorial"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "live-server": "^1.2.2",
        "npm-run-all": "^4.1.5",
        "sass": "^1.69.5"
    }
}
